{% extends '::base.html.twig' %}
{% block _body %}  <h2>Consultas</h2> {% endblock %}
{% block body %}
    
    <h2>Ver</h2>
    <br>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
              <tr>
                <th>Paciente</th>
                <td>{{ entity.paciente | title  }}</td>
            </tr>
            <tr>
                <th>Charla</th>
                <td>{{ entity.charla }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Egreso</th>
                <td>{% if entity.egreso==1 %}{{ "Si" }}{% else %}{{ "No" }}{% endif %}</td>
            </tr>
                     
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('consultas') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('consultas_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
