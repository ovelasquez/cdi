{% extends '::base.html.twig' %}
{% block _body %}  <h2>Medicamentos Suministrados</h2> {% endblock %}
{% block body %}
    
    <h2>Ver</h2>
    <br>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            
             <tr>
                <th>Medicamento</th>
                <td>{{ entity.medicamento }}</td>
            </tr>
              <tr>
                <th>Tipo de Medicamento</th>
                <td>{{ entity.medicamento.tipoMedicamento }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ entity.cantidad }}</td>
            </tr>
            <tr>
                <th>Via de Administracion</th>
                <td>{{ entity.viaAdministracion  | title }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('medicamentossuministrados') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('medicamentossuministrados_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
