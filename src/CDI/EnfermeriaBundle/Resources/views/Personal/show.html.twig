{% extends '::base.html.twig' %}
{% block _body %}  <h2>Personal</h2> {% endblock %}
{% block body %}
    
    <h2>Ver</h2>
    <br>
   
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Procedencia</th>
                <td>{{ entity.procedencia | title }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ entity.tipo | title }}</td>
            </tr>
            <tr>
                <th>Sas</th>
                <td>{{ entity.sas }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('personal') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('personal_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
